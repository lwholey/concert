<table class="profile">
  <tr>
    <td class="main">
      <% unless @user.results.empty? %>
        <table class="results" summary="Results of Search">
	    <tr>
	      <td><b>Event</b></td>
	      <td><b>Date/Time</b></td>
	      <td><b>Venue</b></td>
	      <td><b>Band Name (Spotify)</b></td>
	      <td><b>Track (Spotify)</b></td>
	    </tr>
          <%= render @results %>
        </table>
        <%= will_paginate @results %>
      <% end %>
    </td>
  </tr>
</table>
