<font size="5">lennylonglegs - create concert calendar and playlist</font> </br></br>
<%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages' %>
  <div class="field">
    <% str = 'City'%>
    <%= f.label str %>
    <i class="quiet small">(Boston, 94035, ... )</i><br/>
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <% str = 'Dates'%>
    <%= f.label str %>
    <i class="quiet small">(Future, Past, Today, This week, Last week, Next week, October, ... )</i><br/>
    <%= f.text_field :dates %>
  </div>
  <div class="field">
    <% str = 'Keywords'%>
    <%= f.label str %>
    <i class="quiet small">(Jazz, The Fillmore, Red Rocks, ... )</i><br/>
    <%= f.text_field :keywords %>
  </div>
  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>
<div align="right"> <img src="/images/LLL.png" alt="LLL" WIDTH="25" HEIGHT="25" /> </div>
