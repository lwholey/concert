<font size="5">lennylonglegs - create concert calendar and playlist</font> </br></br>
<%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages' %>
  <div class="field">
	  <% str = 'City'%>
    <%= f.label str %><br />
    <%= f.text_field :city %>
  </div>
  <div class="field">
	  <% str = 'Dates (e.g. Future, Past, Today, This week, Last week, Next week, October)'%>
    <%= f.label str %><br />
    <%= f.text_field :dates %>
  </div>
	<div class="field">
	  <% str = 'Keywords (e.g. Jazz, The Fillmore, Red Rocks)'%>
    <%= f.label str %><br />
    <%= f.text_field :keywords %>
  </div>
  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>
<div align="right"> <img src="/images/LLL.png" alt="LLL" WIDTH="25" HEIGHT="25" /> </div>
